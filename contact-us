<script> var __INITIAL_COUNTRY = "in";
var __UTILSCRIPT = "https://dusyzh85wmzqh.cloudfront.net/frontend/js/utils.min.js";
var __SETNUMBER = ""; </script>
<script> commands.push(function () {
    $(document).ready(function () {
        var cont_Tel = $("#txtPhone");
        cont_Tel.intlTelInput({initialCountry: __INITIAL_COUNTRY, utilsScript: __UTILSCRIPT});
        cont_Tel.intlTelInput("setNumber", __SETNUMBER);
        $('form[name=contactus-form]').submit(function (event) {
            var responeRecaptcha = grecaptcha.getResponse();
            var valid = true;
            if ($('select[name=slReason]').val() == '0') {
                valid = false;
                $('.msg-reason').text('Please Select a topics.');
                $('select[name=slReason]').parent().addClass("has-error");
            } else {
                $('.msg-reason').text('');
                $('select[name=slReason]').parent().removeClass("has-error");
            }
            if ($('input[name=txtName]').val() == '') {
                valid = false;
                $('.msg-name').text('Please Enter Your Full Name.');
                $('input[name=txtName]').parent().addClass("has-error");
            } else {
                $('.msg-name').text('');
                $('input[name=txtName]').parent().removeClass("has-error");
            }
            if ($('input[name=txtEmail]').val() == '') {
                valid = false;
                $('.msg-email').text('Please Enter a email.');
                $('input[name=txtEmail]').parent().addClass("has-error");
            } else if (!emailValid($('input[name=txtEmail]').val())) {
                valid = false;
                $('.msg-email').text('Email is invalid.');
                $('input[name=txtEmail]').parent().addClass("has-error");
            } else {
                $('.msg-email').text('');
                $('input[name=txtEmail]').parent().removeClass("has-error");
            }
            if (cont_Tel.prop("value") < 1) {
                $(".msg-phone").text('Please Enter a phone number.');
                cont_Tel.addClass("border-red");
                valid = false;
            } else {
                $(".msg-phone").text('');
                $('input[name=txtPhone]').parent().removeClass("has-error");
            }
            if (!cont_Tel.intlTelInput("isValidNumber")) {
                $(".msg-phone").text('Your phone number is invalid.');
                $('input[name=txtPhone]').parent().addClass("has-error");
                valid = false;
            } else {
                var countryData = cont_Tel.intlTelInput("getSelectedCountryData");
                var phoneNum = cont_Tel.intlTelInput("getNumber");
                $('#txtTelapiInternationalFormat').val(phoneNum);
                $('#txtTelapiCountryCode').val(countryData.iso2);
                $('input[name=txtPhone]').parent().removeClass("has-error");
            }
            if ($('input[name=txtSubject]').val() == '') {
                valid = false;
                $('.msg-subject').text('Please Enter subject.');
                $('input[name=txtSubject]').parent().addClass("has-error");
            } else {
                $('.msg-subject').text('');
                $('input[name=txtSubject]').parent().removeClass("has-error");
            }
            if ($('textarea#txtContent').val().trim() == '') {
                valid = false;
                $('.msg-content').text('Please Enter a message.');
                $('textarea[name=txtContent]').parent().addClass("has-error");
            } else {
                $('.msg-content').text('');
                $('textarea[name=txtContent]').parent().removeClass("has-error");
            }
            if (responeRecaptcha.length == 0) {
                $(".msg-captcha").text('Please click on the reCAPTCHA box.');
                valid = false;
            } else {
                $(".msg-captcha").text('');
            }
            if (!valid) {
                event.preventDefault();
            }
            if (valid) {
                $('#l-loading').show();
                $('button[name=btnContact]').prop('disabled', true);
            }
        });
        $('input[type=file]').change(function () {
            var arrayFileUpload = [];
            arrayFileUpload[0] = $("#txtAttackFile1");
            arrayFileUpload[1] = $("#txtAttackFile2");
            arrayFileUpload[2] = $("#txtAttackFile3");
            for (var i = 0; i < 3; i++) {
                var countFile = arrayFileUpload[i].get(0).files.length;
                if (countFile == 1) {
                    var fileName = arrayFileUpload[i].get(0).files[0].name;
                    var oneFileSize = arrayFileUpload[i].get(0).files[0].size;
                    var ext = fileName.split('.').pop().toLowerCase();
                    if ($.inArray(ext, ['jpg', 'jpeg', 'png', 'gif', 'bmp', 'pdf', 'txt', 'doc', 'docx', 'xls', 'xlsx']) === -1) {
                        $(this).val('');
                        alert('Please select correct file format.');
                    }
                    if (oneFileSize >= 20971520) {
                        $(this).val('');
                        alert('Please select file size 20MB limit.');
                    }
                    var files = arrayFileUpload[i].prop("files");
                    var names = $.map(files, function (val, index) {
                        return val.name + '<br>';
                    });
                    $('#getname_file' + (i + 1)).html(names);
                }
            }
        });
        var map;
        $("#slAreas").change(function () {
            var area_id = $("#slAreas option:selected").val();
            if (area_id > 0) {
                $.ajax({
                    type: "POST",
                    cache: false,
                    url: "/contact-us/send-jurisdiction",
                    data: {'area_id': area_id},
                    success: function (data) {
                        var getData = JSON.parse(data);
                        $("#slJurisdiction").html('<option value="">Select a country</option>');
                        $("#slJurisdiction").append(getData.string_json);
                    }
                });
            }
        });

                <script type="text/javascript"> var kMethodDirect = 'credit_card';
                var kMethodPaypal = 'paypal';
                var urlGetStates = '/make-payment?action=getStates';
                var methodID = kMethodDirect;
                var isSubmited = false;

                function checkSpecialCharacter(value) {
                    return value.indexOf('(') !== -1 || value.indexOf(')') !== -1 || value.indexOf('{') !== -1 || value.indexOf('}') !== -1 || value.indexOf('<') !== -1 || value.indexOf('>') !== -1 || value.indexOf('\\') !== -1 || value.indexOf('"') !== -1 || value.indexOf(';') !== -1;
                }

                function is_number(s) {
                    var isWhole_re = /^\s*\d+\s*$/;
                    return String(s).search(isWhole_re) !== -1;
                }

                function prettyCardNumber(s) {
                    return s.replace(/\-/g, '').replace(/ /g, "");
                }

                function enableBillingInfo(value) {
                    $('#input_first_name').prop('disabled', value);
                    $('#input_last_name').prop('disabled', value);
                    $('#input_country').prop('disabled', value);
                    $('#input_address').prop('disabled', value);
                    $('#input_city').prop('disabled', value);
                    $('#input_state').prop('disabled', value);
                    $('#slb_states').prop('disabled', value);
                    $('#input_zipcode').prop('disabled', value);
                    $('#input_card_number').prop('disabled', value);
                    $('#input_expired_month').prop('disabled', value);
                    $('#input_expired_year').prop('disabled', value);
                    $('#input_card_cvv2').prop('disabled', value);
                }

                function checkAndMarkInput(input) {
                    var correct = true;
                    input.parent().find('.error_input').addClass("alert alert-danger");
                    if (input.val().trim() === '') {
                        input.parent().find('.error_input').html("This field is required!");
                        correct = false;
                    } else if (checkSpecialCharacter(input.val())) {
                        input.parent().find('.error_input').html("This field contain special characters.");
                        correct = false;
                    } else {
                        input.parent().find('.error_input').removeClass("alert alert-danger");
                        input.parent().find('.error_input').html('');
                        input.parent().removeClass("has-error");
                    }
                    return correct;
                }

                function checkAndMarkInputExpired(input_month, input_year) {
                    var correct = true;
                    var month_parent_parent = input_month.parent().parent();
                    month_parent_parent.find('.error_input').html('');
                    month_parent_parent.find('.error_input').addClass("alert alert-danger");
                    if (input_month.val().trim() === '' && input_year.val().trim() === '') {
                        month_parent_parent.find('.error_input').html("Month & year are required");
                        correct = false;
                    } else if (input_month.val().trim() === '') {
                        month_parent_parent.find('.error_input').html("Month is required");
                        correct = false;
                    } else if (input_year.val().trim() === '') {
                        month_parent_parent.find('.error_input').html("Year is required");
                        correct = false;
                    } else {
                        month_parent_parent.find('.error_input').removeClass("alert alert-danger");
                        month_parent_parent.find('.error_input').html('');
                    }
                    return correct;
                }

                function checkAndMarkInputCardNumber(input) {
                    var $creditcard_master = $('.creditcard_master');
                    var $creditcard_visa = $('.creditcard_visa');
                    var $creditcard_amex = $('.creditcard_amex');
                    var correct = true;
                    input.parent().find('.error_input').html('');
                    input.parent().find('.error_input').addClass("alert alert-danger");
                    var pretty_val = prettyCardNumber(input.val());
                    $('.creditcard_master').css('opacity', 1);
                    $('.creditcard_visa').css('opacity', 1);
                    $('.creditcard_amex').css('opacity', 1);
                    if (pretty_val == '') {
                        input.parent().find('.error_input').html("This field is required!");
                        correct = false;
                    } else if (!checkCreditCard(pretty_val, "Visa") && !checkCreditCard(pretty_val, "MasterCard") && !checkCreditCard(pretty_val, "AmEx")) {
                        $('.creditcard_master').css('opacity', 0.3);
                        $('.creditcard_visa').css('opacity', 0.3);
                        $('.creditcard_amex').css('opacity', 0.3);
                        correct = false;
                        input.parent().find('.error_input').html("Card number is invalid");
                    } else {
                        $('.creditcard_master').css('opacity', 0.3);
                        $('.creditcard_visa').css('opacity', 0.3);
                        $('.creditcard_amex').css('opacity', 0.3);
                        if (checkCreditCard(pretty_val, "Visa")) {
                            $('.creditcard_visa').css('opacity', 1);
                        } else if (checkCreditCard(pretty_val, "MasterCard")) {
                            $('.creditcard_master').css('opacity', 1);
                        } else if (checkCreditCard(pretty_val, "AmEx")) {
                            $('.creditcard_amex').css('opacity', 1);
                        }
                        input.parent().find('.error_input').removeClass("alert alert-danger");
                        input.parent().find('.error_input').html('');
                    }
                    return true;
                }

                function checkAndMarkInputCardCVV(input) {
                    var correct = true;
                    input.parent().find('.error_input').addClass("alert alert-danger");
                    if (input.val() === '') {
                        input.parent().find('.error_input').html("This field is required!");
                        correct = false;
                    } else if (!is_number(input.val()) || (input.val().length != 3 && input.val().length != 4)) {
                        input.parent().find('.error_input').html('CVV not correct');
                        correct = false;
                    } else {
                        input.parent().find('.error_input').removeClass("alert alert-danger");
                        input.parent().find('.error_input').html('');
                    }
                    return correct;
                }

                function addPaymentMethodToForm($form, methodId) {
                    $form.append($('<input type="hidden" name="pay-method">').val(methodId));
                }

                function checkBillingInformation(e) {
                    if (isSubmited) {
                        e.preventDefault();
                        return;
                    }
                    lLoading.show();
                    var $formMessage = $('#form-message');
                    $formMessage.hide();
                    var correct = true;
                    $('#billing_info').find('.error_input').html('');
                    correct = correct & checkAndMarkInput($('#input_first_name'));
                    correct = correct & checkAndMarkInput($('#input_last_name'));
                    correct = correct & checkAndMarkInput($('#input_country'));
                    correct = correct & checkAndMarkInput($('#input_address'));
                    correct = correct & checkAndMarkInput($('#input_city'));
                    correct = correct & checkAndMarkInput($('#input_zipcode'));
                    correct = correct & checkAndMarkInputCardNumber($('#input_card_number'));
                    correct = correct & checkAndMarkInputCardCVV($('#input_card_cvv2'));
                    correct = correct & checkAndMarkInputExpired($('#input_expired_month'), $('#input_expired_year'));
                    var country = $('#input_country').val();
                    var arrCountry = ['US', 'CA', 'IT', 'NL'];
                    if (jQuery.inArray(country, arrCountry) != -1) {
                        correct = correct & checkAndMarkInput($('#slb_states'));
                    } else {
                        correct = correct & checkAndMarkInput($('#input_state'));
                    }
                    var card_type = '';
                    var pretty_val = $('#input_card_number').val().replace(/\-/g, '');
                    if (checkCreditCard(pretty_val, "Visa")) {
                        card_type = 'Visa';
                    } else if (checkCreditCard(pretty_val, "MasterCard")) {
                        card_type = 'MasterCard';
                    } else if (checkCreditCard(pretty_val, "AmEx")) {
                        card_type = 'AmEx';
                    } else {
                        correct = false;
                    }
                    var $form = $('#payment-form');
                    if (correct) {
                        addPaymentMethodToForm($form, methodID);
                        $form.append($('<input type="hidden" name="billing_first_name">').val($('#input_first_name').val()));
                        $form.append($('<input type="hidden" name="billing_last_name">').val($('#input_last_name').val()));
                        $form.append($('<input type="hidden" name="billing_country">').val($('#input_country').val()));
                        $form.append($('<input type="hidden" name="billing_country_name">').val($('#input_country').find("option:selected").text()));
                        $form.append($('<input type="hidden" name="billing_address1">').val($('#input_address').val()));
                        $form.append($('<input type="hidden" name="billing_city">').val($('#input_city').val()));
                        $form.append($('<input type="hidden" name="billing_zip">').val($('#input_zipcode').val()));
                        $form.append($('<input type="hidden" name="card_number">').val(prettyCardNumber($('#input_card_number').val())));
                        $form.append($('<input type="hidden" name="card_expired">').val($('#input_expired_month').val() + $('#input_expired_year').val()));
                        $form.append($('<input type="hidden" name="card_expired_month">').val($('#input_expired_month').val()));
                        $form.append($('<input type="hidden" name="card_expired_year">').val($('#input_expired_year').val()));
                        $form.append($('<input type="hidden" name="card_cvv2">').val($('#input_card_cvv2').val()));
                        if ($.inArray(country, arrCountry) != -1) {
                            $form.append($('<input type="hidden" name="billing_state">').val($('#slb_states').val()));
                        } else {
                            $form.append($('<input type="hidden" name="billing_state">').val($('#input_state').val()));
                        }
                        $form.append($('<input type="hidden" name="card_type">').val(card_type));
                        enableBillingInfo(true);
                        isSubmited = true;
                    } else {
                        lLoading.hide();
                        e.preventDefault();
                        $formMessage.show();
                    }
                }

                function formatState(state) {
                    if (!state.id) {
                        return state.text;
                    }
                    return $('<span><img src="' + state.element.getAttribute('data-img-src') + '" class="img-flag" alt="' + state.text + '"/>' + state.text + '</span>');
                }

                commands.push(function () {
                    $(document).ready(function () {
                        $('#currency').select2({templateResult: formatState, templateSelection: formatState});
                        $('.payment-method-option').click(function () {
                            var $input = $(this).find('input:radio');
                            methodID = $input.val();
                        });
                        $('#input_first_name').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_last_name').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_country').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_address').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_city').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_state').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_zipcode').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#slb_states').blur(function () {
                            checkAndMarkInput($(this));
                        });
                        $('#input_card_number').blur(function () {
                            checkAndMarkInputCardNumber($(this));
                        });
                        $('#input_card_cvv2').blur(function () {
                            checkAndMarkInputCardCVV($(this));
                        });
                        $('#input_card_number').keyup(function () {
                            var cardNumber = $('#input_card_number').val().trim();
                            if (cardNumber.length > 0) {
                                $('.creditcard_visa').css('opacity', 0.3);
                                $('.creditcard_master').css('opacity', 0.3);
                                $('.creditcard_amex').css('opacity', 0.3);
                                if (cardNumber.indexOf('4') == 0) {
                                    $('.creditcard_visa').css('opacity', 1);
                                } else if (cardNumber.indexOf('5') == 0) {
                                    $('.creditcard_master').css('opacity', 1);
                                } else if (cardNumber.indexOf('3') == 0) {
                                    $('.creditcard_amex').css('opacity', 1);
                                }
                            } else {
                                $('.creditcard_master').css('opacity', 1);
                                $('.creditcard_visa').css('opacity', 1);
                                $('.creditcard_amex').css('opacity', 1);
                            }
                        });
                        $('#input_expired_month').change(function () {
                            checkAndMarkInputExpired($('#input_expired_month'), $('#input_expired_year'));
                        });
                        $('#input_expired_year').change(function () {
                            checkAndMarkInputExpired($('#input_expired_month'), $('#input_expired_year'));
                        });
                        var _please_select = "Please select";
                        $('#input_country').change(function () {
                            var country = $('#input_country').val();
                            if (country) {
                                $.ajax({
                                    type: 'POST',
                                    url: urlGetStates,
                                    data: {country_code: country},
                                    success: function (html) {
                                        if (html.length > 1) {
                                            $('.row_state').hide();
                                            $('#slb_states').html('<option value="">' + _please_select + '</option>' + html);
                                            $('.row_states').show();
                                        } else {
                                            $('#input_state').val('');
                                            $('.row_state').show();
                                            $('.row_states').hide();
                                        }
                                    }
                                });
                            }
                        });
                        var amount = $("#id_amount");
                        var isValidAmount = true;
                        var isValidPayFor = true;
                        var totalPayFor = 0;
                        var note = $("#id_note");
                        var isValidNote = true;
                        var formMsg = $("#form-message");
                        var checkboxPayment = $(".checkbox-payment-for");
                        var checkboxPoint = $("#use_oneibc_point");
                        var message = "";
                        amount.on("change blur", function () {
                            var self = $(this);
                            var value = self.prop("value");
                            if (LValidator.validNumber(value) == false) {
                                $("#amount-error").text("Enter a number!");
                                $("#amount-error").addClass("alert alert-danger");
                                isValidAmount = false;
                            } else {
                                $("#amount-error").text("");
                                $("#amount-error").removeClass("alert alert-danger");
                                isValidAmount = true;
                            }
                        });
                        note.on("change blur", function () {
                            var self = $(this);
                            var value = self.prop("value");
                            if (value.length < 1) {
                                $('#note-error').text("This field is required!");
                                $('#note-error').addClass("alert alert-danger");
                                isValidNote = false;
                            } else {
                                $('#note-error').text("");
                                $('#note-error').removeClass("alert alert-danger");
                                isValidNote = true;
                            }
                        });
                        checkboxPayment.on("change blur", function () {
                            totalPayFor = $(':checkbox:checked').length;
                            if (totalPayFor > 0) {
                                $('#payment-for-error').text("");
                                $('#payment-for-error').removeClass("alert alert-danger");
                            } else {
                                $('#payment-for-error').text("Please check the type of your payment!");
                                $('#payment-for-error').addClass("alert alert-danger");
                            }
                        });
                        checkboxPoint.on("change", function () {
                            if ($('#use_oneibc_point').prop("checked") && $('#currency').val() != "USD") {
                                $('#checkpoint-for-error').text("Invalid currency. Please select currency USD to use One IBC Credit points!");
                                $('#checkpoint-for-error').addClass("alert alert-danger");
                            } else {
                                $('#checkpoint-for-error').text("");
                                $('#checkpoint-for-error').removeClass("alert alert-danger");
                            }
                        });
                        var validateForm = function () {
                            amount.trigger("blur");
                            note.trigger("blur");
                            var checked = false;
                            checkboxPayment.each(function () {
                                var self = $(this);
                                if (self.prop("checked")) {
                                    formMsg.hide();
                                    checked = true;
                                    isValidPayFor = true;
                                    return false;
                                }
                            });
                            if (!checked) {
                                $('#payment-for-error').text("Please check the type of your payment!");
                                $('#payment-for-error').addClass("alert alert-danger");
                                isValidPayFor = false;
                            }
                            var point = true;
                            if ($('#use_oneibc_point').prop("checked") && $('#currency').val() != "USD") {
                                $('#checkpoint-for-error').text("Invalid currency. Please select currency USD to use One IBC Credit points!");
                                $('#checkpoint-for-error').addClass("alert alert-danger");
                                point = false;
                            } else {
                                $('#checkpoint-for-error').text("");
                                $('#checkpoint-for-error').removeClass("alert alert-danger");
                                point = true;
                            }
                            return isValidAmount && isValidNote && checked && point;
                        };
                        $('#payment-form').submit(function (event) {
                            var validateSuccess = validateForm();
                            if (!validateSuccess) {
                                message = "Please review and correct the required field(s)!";
                                formMsg.html(message);
                                formMsg.addClass("alert alert-danger");
                                formMsg.show();
                                event.preventDefault();
                                return;
                            }
                            switch (methodID) {
                                case kMethodDirect:
                                    checkBillingInformation(event);
                                    break;
                                case kMethodPaypal:
                                    lLoading.show();
                                    var $form = $('#payment-form');
                                    addPaymentMethodToForm($form, methodID);
                                    break;
                                default:
                                    break;
                            }
                        });
                    });
                }); </script>
            </div>
        </div>
    </div>
</div>

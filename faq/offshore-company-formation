<script> function loadmore(type) {
    var row = Number($('#row_' + type).val());
    var allcount = Number($('#all_' + type).val());
    var limit = Number(5);
    if ((row != 'NaN') && (allcount != 'NaN')) {
        row = row + limit;
        if (row <= allcount) {
            $("#row_" + type).val(row);
            $.ajax({
                url: "/faq/getdata",
                type: 'post',
                data: {type: type, row: row, limit: limit},
                success: function (response) {
                    $(".post_" + type + ":last").after(response).show();
                    var rowno = row + limit;
                    if (rowno > allcount) {
                        $('#load_' + type).addClass("hide");
                    }
                }
            });
        }
    }
}; </script>

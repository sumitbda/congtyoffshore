<script> commands.push(function () {
    $(document).ready(function () {
        var regisTel = $('input[name=txtPhone]');
        regisTel.intlTelInput({
            initialCountry: "in",
            preferredCountries: ["in"],
            formatOnDisplay: true,
            utilsScript: "https://dusyzh85wmzqh.cloudfront.net/frontend/js/utils.min.js"
        });
        regisTel.intlTelInput("setNumber", "");
        var isValidNumber = true;
        regisTel.blur(function () {
            var phone_number = regisTel.intlTelInput('getNumber');
            $('input[name=txt_phone_number]').val(phone_number);
            isValidNumber = regisTel.intlTelInput('isValidNumber');
        });
        var country_data = regisTel.intlTelInput('getSelectedCountryData');
        $('input[name=txt_iso2]').val(country_data.iso2);
        $('form[name=register-form]').submit(function (event) {
            var valid = true;
            if ($('input[name=txtName]').val() == '') {
                valid = false;
                $('.msg-name').text('Please Enter Your Full Name.');
                $('input[name=txtName]').parent().addClass("has-error");
            } else {
                $('.msg-name').text('');
                $('input[name=txtName]').parent().removeClass("has-error");
            }
            if ($('input[name=txtEmail]').val() == '') {
                valid = false;
                $('.msg-email').text('Please Enter a email.');
                $('input[name=txtEmail]').parent().addClass("has-error");
            } else if (!emailValid($('input[name=txtEmail]').val())) {
                valid = false;
                $('.msg-email').text('Email is invalid.');
                $('input[name=txtEmail]').parent().addClass("has-error");
            } else {
                $('.msg-email').text('');
                $('input[name=txtEmail]').parent().removeClass("has-error");
            }
            if ($('input[name=txtPassword]').val() == '') {
                valid = false;
                $('input[name=txtPassword]').parent().addClass("has-error");
                $('.msg-pass').text('Please enter password.');
            } else {
                $('.msg-pass').text('');
                $('input[name=txtPassword]').parent().removeClass("has-error");
            }
            if ($('input[name=txtCPassword]').val() == '') {
                $('input[name=txtCPassword]').parent().addClass("has-error");
                $('.msg-repass').text('Please enter confirm Password.');
                valid = false;
            } else {
                if ($('input[name=txtCPassword]').val() !== $('input[name=txtPassword]').val()) {
                    $('input[name=txtPassword]').parent().addClass("has-error");
                    $('input[name=txtCPassword]').parent().addClass("has-error");
                    $('.msg-repass').text('Confirm Password is not match.');
                    valid = false;
                } else {
                    $('.msg-repass').text('');
                    $('input[name=txtCPassword]').parent().removeClass("has-error");
                    $('input[name=txtPassword]').parent().removeClass("has-error");
                }
            }
            if ($('input[name=txtPhone]').val() == '') {
                valid = false;
                $('.msg-phone').text('Please Enter a phone number.');
                $('input[name=txtPhone]').parent().addClass("has-error");
            } else if (!isValidNumber) {
                valid = false;
                $('.msg-phone').text('Your phone number is invalid.');
                $('input[name=txtPhone]').parent().addClass("has-error");
            } else {
                $('.msg-phone').text('');
                $('input[name=txtPhone]').parent().removeClass("has-error");
            }
            if ($("select#slcAddressCountry option:checked").val() == 0) {
                $('.msg-address-country').text('Please enter shipping address country.');
                $('#slcAddressCountry').parent().addClass("has-error");
                valid = false;
            } else {
                $('.msg-address-country').text('');
                $('#slcAddressCountry').parent().removeClass("has-error");
            }
            if ($('input[name=txtAddress]').val() == '') {
                valid = false;
                $('.msg-address').text('Type your address');
                $('input[name=txtAddress]').parent().addClass("has-error");
            } else {
                $('.msg-address').text('');
                $('input[name=txtAddress]').parent().removeClass("has-error");
            }
            if (!valid) {
                event.preventDefault();
            }
            if (valid) {
                $('#l-loading').show();
                $('button[name=btnCreateaccount]').prop('disabled', true);
            }
        });
        $("input[name=txtPassword]").keypress(function (e) {
            var s = String.fromCharCode(e.which);
            if (s.toUpperCase() === s && s.toLowerCase() !== s && !e.shiftKey) {
                $(".warning-capslock").show();
            } else {
                $(".warning-capslock").hide();
            }
        });
    });
}); </script>

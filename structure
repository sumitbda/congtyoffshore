<script type="text/javascript"> function formatState(state) {
    if (!state.id) {
        return state.text;
    }
    var $state = $('<span><img src="' + state.element.getAttribute('data-img-src') + '" class="img-flag" title="' + state.text.trim() + '" alt="' + state.text.trim() + '" /> ' + state.text + '</span>');
    return $state;
}

$("#selCurrency").select2({templateResult: formatState, templateSelection: formatState}); </script>

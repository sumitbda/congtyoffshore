<script type="text/javascript"> var orderServicesVM = null;
var total = null;
var activeAjaxConnections = 0;
commands.push(function () {
    orderServicesVM = createOrderServicesVM();
    total = createTotal(orderServicesVM);
    $(function () {
        ko.bindingHandlers.fadeVisibleAlert = {
            init: function (element, valueAccessor) {
            }, update: function (element, valueAccessor, allBindings) {
                var value = valueAccessor();
                var valueUnwrapped = ko.unwrap(value);
                var className1 = allBindings.get('className1') || '';
                var className2 = allBindings.get('className2') || '';
                if (valueUnwrapped == true) {
                    $(element).addClass(className1);
                    $(element).removeClass(className2);
                } else {
                    $(element).removeClass(className1);
                    $(element).addClass(className2);
                }
            }
        };
        ko.applyBindings(orderServicesVM, document.getElementById("step-main-content"));
    });
}); </script>

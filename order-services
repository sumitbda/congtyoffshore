                <script type="text/javascript"> var MAX_OF_DIRECTORS = 15;
                var MAX_OF_SHAREHOLDERS = 15;
                var MAX_OF_TRUSTORS = 15;
                var MAX_OF_TRUSTEES = 15;
                var MAX_OF_COUNCILS = 15;
                var MAX_OF_FOUNDERS = 15;
                var defaultCheckedNominee = false, defaultCheckedServiceOffice = false,
                    defaultCountryIdForServiceOffice = (0).toString(), defaultCheckedBookkeeping = false,
                    defaultBookkeepingIdForBookkeeping = (0).toString(), defaultCheckedMerchant = false,
                    defaultCheckedIPService = false, defaultCheckedAccounting = false;
                commands.push(function () {
                    lLoading.show();
                    $(function () {
                        orderServicesVM.globalData.getSession();
                        var e = $("#type-of-service");
                        var K = $("#incorporation-country, #incorporation-country-2");
                        var i = $("#type-of-incorporate, #type-of-incorporate-2");
                        var d = $("#number-of-directors, #number-of-directors-2, #number-of-directors-3");
                        var I = $("#number-of-shareholders, #number-of-shareholders-2, #number-of-shareholders-3");
                        var G = $("#processing-time-incorporation");
                        var q = $("#trust-country");
                        var x = $("#number-of-trustors");
                        var b = $("#number-of-trustees");
                        var j = $("#processing-time-trust");
                        var n = $("#foundation-country");
                        var r = $("#number-of-councils");
                        var u = $("#number-of-founders");
                        var z = $("#processing-time-foundation");
                        var J = $("#bank-country");
                        var w = $("#bank-name");
                        var y = $("#bank-with-setup-company");
                        var v = $("#bank-without-setup-company");
                        var C = $("#processing-time-bank");
                        var p = $("#incorporation-country-3");
                        var L = $("#nominee-checkbox");
                        var o = $("#nomidir-number");
                        var a = $("#nomishare-number");
                        var D = $("#nomi-trustor-number");
                        var O = $("#nomi-trustee-number");
                        var s = $("#nomi-council-number");
                        var A = $("#nomi-founder-number");
                        var k = $("#open-bank-account-checkbox");
                        var m = $("#bank-country-on-checkbox");
                        var h = $("#bank-name-on-checkbox");
                        var t = $("#office-checkbox");
                        var H = $("#office-country");
                        var M = $("#office-service-period");
                        var N = $("#bookkeeping-checkbox");
                        var g = $("#bookkeeping");
                        var l = $("#bookkeeping-period");
                        var F = $("#merchant-account-online-checkbox");
                        var f = $("#ip-services-checkbox");
                        var B = $("#accounting-checkbox");
                        var E = $("#promotion-code");
                        var c = $("#error-promotion-code");
                        e.on("change", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var R = P.find("option:selected");
                            var Q = R.text();
                            orderServicesVM.globalData.typeOfServiceValue(S);
                            orderServicesVM.globalData.typeOfServiceName(Q);
                            orderServicesVM.Incorporation.isActive(false);
                            orderServicesVM.ProcessingIncorporation.isActive(false);
                            orderServicesVM.NomiDir.isActive(false);
                            orderServicesVM.NomiShare.isActive(false);
                            orderServicesVM.Trust.isActive(false);
                            orderServicesVM.ProcessingTrust.isActive(false);
                            orderServicesVM.NomiTrustor.isActive(false);
                            orderServicesVM.NomiTrustee.isActive(false);
                            orderServicesVM.Foundation.isActive(false);
                            orderServicesVM.ProcessingFoundation.isActive(false);
                            orderServicesVM.NomiCouncil.isActive(false);
                            orderServicesVM.NomiFounder.isActive(false);
                            orderServicesVM.Bank.isActive(false);
                            orderServicesVM.ProcessingBank.isActive(false);
                            switch (S) {
                                case"":
                                    break;
                                case"Company":
                                    orderServicesVM.Incorporation.isActive(true);
                                    orderServicesVM.ProcessingIncorporation.isActive(true);
                                    orderServicesVM.Incorporation.getCountries();
                                    if (orderServicesVM.Incorporation.getCountryRequest != null) {
                                        orderServicesVM.Incorporation.getCountryRequest.done(function () {
                                            K.trigger("change")
                                        })
                                    }
                                    orderServicesVM.NomiDir.isActive(true);
                                    orderServicesVM.NomiShare.isActive(true);
                                    break;
                                case"Trust":
                                    orderServicesVM.Trust.isActive(true);
                                    orderServicesVM.ProcessingTrust.isActive(true);
                                    orderServicesVM.Trust.getCountries();
                                    if (orderServicesVM.Trust.getCountryRequest != null) {
                                        orderServicesVM.Trust.getCountryRequest.done(function () {
                                            q.trigger("change")
                                        })
                                    }
                                    orderServicesVM.NomiTrustor.isActive(true);
                                    orderServicesVM.NomiTrustee.isActive(true);
                                    break;
                                case"Foundation":
                                    orderServicesVM.Foundation.isActive(true);
                                    orderServicesVM.ProcessingFoundation.isActive(true);
                                    orderServicesVM.Foundation.getCountries();
                                    if (orderServicesVM.Foundation.getCountryRequest != null) {
                                        orderServicesVM.Foundation.getCountryRequest.done(function () {
                                            n.trigger("change")
                                        })
                                    }
                                    orderServicesVM.NomiCouncil.isActive(true);
                                    orderServicesVM.NomiFounder.isActive(true);
                                    break;
                                case"Bank":
                                    orderServicesVM.Bank.isActive(true);
                                    orderServicesVM.ProcessingBank.isActive(true);
                                    if (orderServicesVM.Bank.getCountryRequest != null) {
                                        orderServicesVM.Bank.getCountryRequest.done(function () {
                                            J.trigger("change")
                                        })
                                    }
                                    orderServicesVM.Incorporation.getCountries();
                                    orderServicesVM.NomiDir.isActive(true);
                                    orderServicesVM.NomiShare.isActive(true);
                                    if (orderServicesVM.globalData.bankWithSetupCompany()) {
                                        y.trigger("click")
                                    } else {
                                        if (orderServicesVM.globalData.bankWithoutSetupCompany()) {
                                            v.trigger("click")
                                        }
                                    }
                                    break;
                                default:
                                    orderServicesVM.globalData.typeOfServiceName("")
                            }
                        });
                        K.on("change", function () {
                            var P = $(this);
                            var R = P.find("option:selected");
                            var S = P.prop("value");
                            var Q = R.text();
                            if (S === "") {
                                S = 0
                            }
                            orderServicesVM.globalData.incorporationCountryID(S);
                            orderServicesVM.globalData.incorporationCountryName(Q);
                            orderServicesVM.Incorporation.getTypes(S);
                            resetTypeOfIncorporation();
                            resetDirectors();
                            resetShareholders();
                            resetProcessingTimeOfIncorporation()
                        });
                        i.on("change", function () {
                            var Q = $(this);
                            var V = Q.prop("value");
                            if (V >= 1) {
                                orderServicesVM.globalData.typeOfIncorporationID(V);
                                var P = orderServicesVM.Incorporation.typesDic();
                                var T = P[V];
                                if (T) {
                                    var S, U;
                                    var W = T.service_is_director == "Y";
                                    var R = T.service_is_shareholder == "Y";
                                    orderServicesVM.globalData.typeOfIncorporationName(T.service_name);
                                    orderServicesVM.globalData.typeOfIncorporationPrice(parseFloatNumber(T.service_price));
                                    orderServicesVM.globalData.isDirector(W);
                                    if (W) {
                                        U = T.service_min_directors;
                                        S = orderServicesVM.globalData.numberOfDirectors();
                                        if (S < U) {
                                            S = U
                                        }
                                        d.html(createNumberOptions(U, MAX_OF_DIRECTORS, S));
                                        d.trigger("change")
                                    } else {
                                        resetDirectors()
                                    }
                                    orderServicesVM.globalData.isShareholder(R);
                                    if (R) {
                                        U = T.service_min_shareholders;
                                        S = orderServicesVM.globalData.numberOfShareholders();
                                        if (S < U) {
                                            S = U
                                        }
                                        I.html(createNumberOptions(U, MAX_OF_SHAREHOLDERS, S));
                                        I.trigger("change")
                                    } else {
                                        resetShareholders()
                                    }
                                    if (orderServicesVM.globalData.typeOfServiceValue() === "Company") {
                                        orderServicesVM.ProcessingIncorporation.getProcessings(orderServicesVM.globalData.incorporationCountryID(), V);
                                        if (orderServicesVM.ProcessingIncorporation.getProcessingRequest != null) {
                                            orderServicesVM.ProcessingIncorporation.getProcessingRequest.done(function () {
                                                G.find(".radio").eq(0).trigger("click");
                                                G.find("input:radio").eq(0).prop("checked", true).trigger("change")
                                            })
                                        }
                                    }
                                }
                            } else {
                                resetTypeOfIncorporation();
                                resetProcessingTimeOfIncorporation();
                                resetDirectors();
                                resetShareholders()
                            }
                        });
                        d.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfDirectors(Q);
                            o.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiDir()));
                            o.trigger("change")
                        });
                        I.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfShareholders(Q);
                            a.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiShare()));
                            a.trigger("change")
                        });
                        q.on("change", function () {
                            var P = $(this);
                            var R = P.find("option:selected");
                            var S = P.prop("value");
                            var Q = R.text();
                            if (S === "") {
                                S = 0
                            }
                            orderServicesVM.globalData.trustCountryID(S);
                            orderServicesVM.globalData.trustCountryName(Q);
                            resetTypeOfTrust();
                            if (S >= 1) {
                                orderServicesVM.Trust.getTypes(S)
                            } else {
                                resetTrustors();
                                resetTrustees()
                            }
                            resetProcessingTimeOfTrust();
                            orderServicesVM.ProcessingTrust.getProcessings(S);
                            if (orderServicesVM.ProcessingTrust.getProcessingRequest != null) {
                                orderServicesVM.ProcessingTrust.getProcessingRequest.done(function () {
                                    j.find(".radio").eq(0).trigger("click");
                                    j.find("input:radio").eq(0).prop("checked", true).trigger("change")
                                })
                            }
                        });
                        x.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfTrustors(Q);
                            D.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiTrustor()));
                            D.trigger("change")
                        });
                        b.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfTrustees(Q);
                            O.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiTrustee()));
                            O.trigger("change")
                        });
                        n.on("change", function () {
                            var P = $(this);
                            var R = P.find("option:selected");
                            var S = P.prop("value");
                            var Q = R.text();
                            if (S === "") {
                                S = 0
                            }
                            orderServicesVM.globalData.foundationCountryID(S);
                            orderServicesVM.globalData.foundationCountryName(Q);
                            resetTypeOfFoundation();
                            if (S >= 1) {
                                orderServicesVM.Foundation.getTypes(S)
                            } else {
                                resetCouncils();
                                resetFounders()
                            }
                            resetProcessingTimeOfFoundation();
                            orderServicesVM.ProcessingFoundation.getProcessings(S);
                            if (orderServicesVM.ProcessingFoundation.getProcessingRequest != null) {
                                orderServicesVM.ProcessingFoundation.getProcessingRequest.done(function () {
                                    z.find(".radio").eq(0).trigger("click");
                                    z.find("input:radio").eq(0).prop("checked", true).trigger("change")
                                })
                            }
                        });
                        r.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfCouncils(Q);
                            s.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiCouncil()));
                            s.trigger("change")
                        });
                        u.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfFounders(Q);
                            A.html(numberOfNominee(Q, orderServicesVM.globalData.numberOfNomiFounder()));
                            A.trigger("change")
                        });
                        J.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            var R = P.find("option:selected").text();
                            if (Q === "") {
                                Q = 0
                            }
                            orderServicesVM.Bank.getBanks(Q);
                            orderServicesVM.globalData.bankCountryID(Q);
                            orderServicesVM.globalData.bankCountryName(R);
                            resetTypeOfBank();
                            resetProcessingTimeOfBank()
                        });
                        w.on("change", function () {
                            var Q = $(this);
                            var R = Q.prop("value");
                            if (R >= 1) {
                                var S = orderServicesVM.Bank.banks();
                                for (var P in S) {
                                    if (S[P]["service_id"] == R) {
                                        orderServicesVM.globalData.bankName(S[P]["service_name"]);
                                        orderServicesVM.globalData.bankPrice(parseFloatNumber(S[P]["service_price"]));
                                        break
                                    }
                                }
                                orderServicesVM.ProcessingBank.getProcessings(orderServicesVM.globalData.bankCountryID(), R);
                                if (orderServicesVM.ProcessingBank.getProcessingRequest != null) {
                                    orderServicesVM.ProcessingBank.getProcessingRequest.done(function () {
                                        C.find(".radio").eq(0).trigger("click");
                                        C.find("input:radio").eq(0).prop("checked", true).trigger("change")
                                    })
                                }
                            } else {
                                resetTypeOfBank();
                                resetProcessingTimeOfBank()
                            }
                        });
                        y.on("click", function () {
                            $("#bank-question-error").hide();
                            var P = $(this);
                            orderServicesVM.globalData.bankWithSetupCompany(true);
                            orderServicesVM.globalData.bankWithoutSetupCompany(false);
                            orderServicesVM.NomiDir.isActive(true);
                            orderServicesVM.NomiShare.isActive(true);
                            resetDirectors();
                            resetShareholders();
                            K.trigger("change")
                        });
                        v.on("click", function () {
                            $("#bank-question-error").hide();
                            $("#nominee").collapse("hide");
                            orderServicesVM.NomiDir.isActive(false);
                            orderServicesVM.NomiShare.isActive(false);
                            var P = $(this);
                            orderServicesVM.globalData.bankWithSetupCompany(false);
                            orderServicesVM.globalData.bankWithoutSetupCompany(true);
                            var S = $("#incorporation-country-3").prop("value");
                            var Q = $("#incorporation-country-3").find("option:selected").text();
                            if (S === "") {
                                S = 0
                            }
                            orderServicesVM.globalData.incorporationCountryID(S);
                            orderServicesVM.globalData.incorporationCountryName(Q);
                            orderServicesVM.globalData.isDirector(true);
                            var R = orderServicesVM.globalData.numberOfDirectors();
                            if (R < 1) {
                                R = 1
                            }
                            d.html(createNumberOptions(1, MAX_OF_DIRECTORS, R));
                            d.trigger("change");
                            orderServicesVM.globalData.isShareholder(true);
                            var R = orderServicesVM.globalData.numberOfShareholders();
                            if (R < 1) {
                                R = 1
                            }
                            I.html(createNumberOptions(1, MAX_OF_SHAREHOLDERS, R));
                            I.trigger("change")
                        });
                        p.on("change", function () {
                            var P = $(this);
                            var R = P.find("option:selected");
                            var S = P.prop("value");
                            var Q = R.text();
                            if (S === "") {
                                S = 0
                            }
                            orderServicesVM.globalData.incorporationCountryID(S);
                            orderServicesVM.globalData.incorporationCountryName(Q)
                        });
                        k.on("change", function () {
                            var P = $(this);
                            if (P.is(":checked")) {
                                orderServicesVM.globalData.selectBank(true);
                                m.trigger("change")
                            } else {
                                orderServicesVM.globalData.selectBank(false);
                                resetCountryOfBank();
                                resetTypeOfBank()
                            }
                        });
                        m.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            var R = P.find("option:selected").text();
                            if (Q === "") {
                                Q = 0
                            }
                            orderServicesVM.Bank.getBanks(Q);
                            orderServicesVM.globalData.bankCountryID(Q);
                            orderServicesVM.globalData.bankCountryName(R);
                            resetTypeOfBank()
                        });
                        h.on("change", function () {
                            var Q = $(this);
                            var R = Q.prop("value");
                            if (R >= 1) {
                                var S = orderServicesVM.Bank.banks();
                                for (var P in S) {
                                    if (S[P]["service_id"] == R) {
                                        orderServicesVM.globalData.bankName(S[P]["service_name"]);
                                        orderServicesVM.globalData.bankPrice(parseFloatNumber(S[P]["service_price"]));
                                        break
                                    }
                                }
                            } else {
                                resetTypeOfBank()
                            }
                        });
                        L.on("change", function () {
                            var P = $(this);
                            if (P.is(":checked")) {
                                orderServicesVM.globalData.selectNominee(true);
                                switch (orderServicesVM.globalData.typeOfServiceValue()) {
                                    case"Bank":
                                    case"Company":
                                        o.trigger("change");
                                        a.trigger("change");
                                        break;
                                    case"Trust":
                                        D.trigger("change");
                                        O.trigger("change");
                                        break;
                                    case"Foundation":
                                        s.trigger("change");
                                        A.trigger("change");
                                        break;
                                    default:
                                }
                            } else {
                                orderServicesVM.globalData.selectNominee(false);
                                resetNominee()
                            }
                        });
                        o.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiDir(Q);
                            orderServicesVM.globalData.totalOfNomiDir(parseFloatNumber(orderServicesVM.globalData.nomiDirPrice() * Q))
                        });
                        a.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiShare(Q);
                            orderServicesVM.globalData.totalOfNomiShare(parseFloatNumber(orderServicesVM.globalData.nomiSharePrice() * Q))
                        });
                        D.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiTrustor(Q);
                            orderServicesVM.globalData.totalOfNomiTrustor(parseFloatNumber(orderServicesVM.globalData.nomiTrustorPrice() * Q))
                        });
                        O.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiTrustee(Q);
                            orderServicesVM.globalData.totalOfNomiTrustee(parseFloatNumber(orderServicesVM.globalData.nomiTrusteePrice() * Q))
                        });
                        s.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiCouncil(Q);
                            orderServicesVM.globalData.totalOfNomiCouncil(parseFloatNumber(orderServicesVM.globalData.nomiCouncilPrice() * Q))
                        });
                        A.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            orderServicesVM.globalData.numberOfNomiFounder(Q);
                            orderServicesVM.globalData.totalOfNomiFounder(parseFloatNumber(orderServicesVM.globalData.nomiFounderPrice() * Q))
                        });
                        t.on("change", function () {
                            if (this.checked) {
                                orderServicesVM.globalData.selectOffice(true);
                                H.trigger("change")
                            } else {
                                orderServicesVM.globalData.selectOffice(false);
                                resetCountryOfOfficePhone()
                            }
                        });
                        H.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            var R = P.find("option:selected").text();
                            if (Q === "") {
                                Q = 0
                            }
                            orderServicesVM.globalData.officeCountryID(Q);
                            orderServicesVM.globalData.officeCountryName(R);
                            orderServicesVM.OfficePhone.getOffices(Q);
                            if (orderServicesVM.OfficePhone.getOfficeRequest != null) {
                                orderServicesVM.OfficePhone.getOfficeRequest.done(function () {
                                    M.find(".radio").eq(0).trigger("click");
                                    M.find("input:radio").eq(0).prop("checked", true).trigger("click").trigger("change")
                                })
                            }
                        });
                        M.on("change", "input:radio", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var Q = P.attr("service-name");
                            var R = P.attr("service-price");
                            orderServicesVM.globalData.officeName(Q);
                            orderServicesVM.globalData.officePrice(parseFloatNumber(R));
                            orderServicesVM.globalData.officeID(S)
                        });
                        N.on("change", function () {
                            if (this.checked) {
                                orderServicesVM.globalData.selectBookkeeping(true);
                                g.trigger("change")
                            } else {
                                orderServicesVM.globalData.selectBookkeeping(false);
                                resetBookkeepings()
                            }
                        });
                        g.on("change", function () {
                            var R = $(this);
                            var S = R.prop("value");
                            var Q = R.find("option:selected").text();
                            if (S === "") {
                                S = 0
                            }
                            if (S >= 1) {
                                orderServicesVM.globalData.bookkeepingID(S);
                                orderServicesVM.globalData.bookkeepingName(Q);
                                var T = orderServicesVM.Bookkeeping.bookkeepings();
                                for (var P in T) {
                                    if (T[P]["service_id"] === S) {
                                        orderServicesVM.globalData.bookkeepingMonthPrice(T[P]["service_price_monthly"]);
                                        orderServicesVM.globalData.bookkeepingQuarterPrice(T[P]["service_price_quarterly"]);
                                        break
                                    }
                                }
                                l.find("input:radio").filter(function () {
                                    return $(this).prop("value") === orderServicesVM.globalData.bookkeepingPeriodValue()
                                }).prop("checked", true).trigger("click").trigger("change")
                            } else {
                                resetBookkeepings()
                            }
                        });
                        l.on("change", "input:radio", function () {
                            var P = $(this);
                            var R = P.prop("value");
                            var Q = P.attr("period-name");
                            orderServicesVM.globalData.bookkeepingPeriodValue(R);
                            orderServicesVM.globalData.bookkeepingPeriodName(Q);
                            if (R === orderServicesVM.Bookkeeping.kMonthly) {
                                orderServicesVM.globalData.bookkeepingPrice(parseFloatNumber(orderServicesVM.globalData.bookkeepingMonthPrice()))
                            } else {
                                if (R === orderServicesVM.Bookkeeping.kQuarterly) {
                                    orderServicesVM.globalData.bookkeepingPrice(parseFloatNumber(orderServicesVM.globalData.bookkeepingQuarterPrice()))
                                }
                            }
                        });
                        F.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            if (P.is(":checked")) {
                                orderServicesVM.globalData.selectMerchantAccountOnline(true);
                                orderServicesVM.globalData.merchantAccountOnlinePrice(parseFloatNumber(orderServicesVM.Merchant.firstMerchantPrice()))
                            } else {
                                orderServicesVM.globalData.selectMerchantAccountOnline(false);
                                orderServicesVM.globalData.merchantAccountOnlinePrice(0)
                            }
                        });
                        f.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            if (P.is(":checked")) {
                                orderServicesVM.globalData.selectIpServices(true);
                                orderServicesVM.globalData.ipServicesPrice(parseFloatNumber(orderServicesVM.IPService.firstIPServicePrice()))
                            } else {
                                orderServicesVM.globalData.selectIpServices(false);
                                orderServicesVM.globalData.ipServicesPrice(0)
                            }
                        });
                        B.on("change", function () {
                            var P = $(this);
                            var Q = P.prop("value");
                            if (P.is(":checked")) {
                                orderServicesVM.globalData.selectAccounting(true);
                                orderServicesVM.globalData.accountingPrice(parseFloatNumber(orderServicesVM.Accounting.firstAccountingPrice()))
                            } else {
                                orderServicesVM.globalData.selectAccounting(false);
                                orderServicesVM.globalData.accountingPrice(0)
                            }
                        });
                        G.on("change", "input:radio", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var Q = P.attr("service-name");
                            var R = P.attr("service-price");
                            orderServicesVM.globalData.incorporationProcessingID(S);
                            orderServicesVM.globalData.incorporationProcessingName(Q);
                            orderServicesVM.globalData.incorporationProcessingPrice(parseFloatNumber(R))
                        });
                        C.on("change", "input:radio", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var Q = P.attr("service-name");
                            var R = P.attr("service-price");
                            orderServicesVM.globalData.bankProcessingID(S);
                            orderServicesVM.globalData.bankProcessingName(Q);
                            orderServicesVM.globalData.bankProcessingPrice(parseFloatNumber(R))
                        });
                        j.on("change", "input:radio", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var Q = P.attr("service-name");
                            var R = P.attr("service-price");
                            orderServicesVM.globalData.trustProcessingID(S);
                            orderServicesVM.globalData.trustProcessingName(Q);
                            orderServicesVM.globalData.trustProcessingPrice(parseFloatNumber(R))
                        });
                        z.on("change", "input:radio", function () {
                            var P = $(this);
                            var S = P.prop("value");
                            var Q = P.attr("service-name");
                            var R = P.attr("service-price");
                            orderServicesVM.globalData.foundationProcessingID(S);
                            orderServicesVM.globalData.foundationProcessingName(Q);
                            orderServicesVM.globalData.foundationProcessingPrice(parseFloatNumber(R))
                        });
                        orderServicesVM.globalData.getSessionRequest.done(function () {
                            orderServicesVM.OfficePhone.isActive(true);
                            orderServicesVM.Bookkeeping.isActive(true);
                            orderServicesVM.Merchant.isActive(true);
                            orderServicesVM.IPService.isActive(true);
                            orderServicesVM.Accounting.isActive(true);
                            orderServicesVM.Bank.getCountries();
                            orderServicesVM.NomiDir.getNominee();
                            if (orderServicesVM.NomiDir.getNomineeRequest != null) {
                                orderServicesVM.NomiDir.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiDirPrice(parseFloatNumber(orderServicesVM.NomiDir.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.NomiShare.getNominee();
                            if (orderServicesVM.NomiShare.getNomineeRequest != null) {
                                orderServicesVM.NomiShare.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiSharePrice(parseFloatNumber(orderServicesVM.NomiShare.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.NomiTrustor.getNominee();
                            if (orderServicesVM.NomiTrustor.getNomineeRequest != null) {
                                orderServicesVM.NomiTrustor.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiTrustorPrice(parseFloatNumber(orderServicesVM.NomiTrustor.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.NomiTrustee.getNominee();
                            if (orderServicesVM.NomiTrustee.getNomineeRequest != null) {
                                orderServicesVM.NomiTrustee.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiTrusteePrice(parseFloatNumber(orderServicesVM.NomiTrustee.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.NomiCouncil.getNominee();
                            if (orderServicesVM.NomiCouncil.getNomineeRequest != null) {
                                orderServicesVM.NomiCouncil.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiCouncilPrice(parseFloatNumber(orderServicesVM.NomiCouncil.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.NomiFounder.getNominee();
                            if (orderServicesVM.NomiFounder.getNomineeRequest != null) {
                                orderServicesVM.NomiFounder.getNomineeRequest.done(function () {
                                    orderServicesVM.globalData.nomiFounderPrice(parseFloatNumber(orderServicesVM.NomiFounder.firstNomineePrice()))
                                })
                            }
                            orderServicesVM.OfficePhone.getCountries();
                            orderServicesVM.Bookkeeping.getBookkeepings();
                            orderServicesVM.Merchant.getMerchants();
                            orderServicesVM.IPService.getIPServices();
                            orderServicesVM.Accounting.getAccounting();
                            e.find("option").filter(function () {
                                return $(this).prop("value") === orderServicesVM.globalData.sessions()["type"]
                            }).prop("selected", true);
                            e.trigger("change");
                            switch (orderServicesVM.globalData.typeOfServiceValue()) {
                                case"Company":
                                    if (orderServicesVM.Incorporation.getCountryRequest != null) {
                                        orderServicesVM.Incorporation.getCountryRequest.done(function () {
                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]) !== "undefined") {
                                                K.find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]["detail_service_id"]]["service_country_id"]
                                                }).prop("selected", true)
                                            } else {
                                                if (typeof (orderServicesVM.globalData.sessions()["countryIdByGetRequest"]) !== "undefined" && orderServicesVM.globalData.sessions()["countryIdByGetRequest"] !== 0) {
                                                    K.find("option").filter(function () {
                                                        return $(this).prop("value") === orderServicesVM.globalData.sessions()["countryIdByGetRequest"]
                                                    }).prop("selected", true)
                                                }
                                            }
                                            K.trigger("change");
                                            if (orderServicesVM.Incorporation.getTypeRequest != null) {
                                                orderServicesVM.Incorporation.getTypeRequest.done(function () {
                                                    if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]) !== "undefined") {
                                                        i.find("option").filter(function () {
                                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]["detail_service_id"]
                                                        }).prop("selected", true)
                                                    }
                                                    orderServicesVM.globalData.numberOfDirectors(orderServicesVM.globalData.sessions()["numberOfDirectors"]);
                                                    orderServicesVM.globalData.numberOfShareholders(orderServicesVM.globalData.sessions()["numberOfShareholders"]);
                                                    orderServicesVM.globalData.numberOfNomiDir(orderServicesVM.globalData.sessions()["numberOfNomiDirectors"]);
                                                    orderServicesVM.globalData.numberOfNomiShare(orderServicesVM.globalData.sessions()["numberOfNomiShareholders"]);
                                                    $("#type-of-incorporate").trigger("change");
                                                    if (orderServicesVM.ProcessingIncorporation.getProcessingRequest != null) {
                                                        orderServicesVM.ProcessingIncorporation.getProcessingRequest.done(function () {
                                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingIncorporation.key]) !== "undefined") {
                                                                G.find("input:radio").filter(function () {
                                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingIncorporation.key]["detail_service_id"]
                                                                }).prop("checked", true).trigger("click").trigger("change")
                                                            }
                                                        })
                                                    }
                                                })
                                            }
                                        })
                                    }
                                    if (orderServicesVM.globalData.selectNominee()) {
                                        if (orderServicesVM.NomiDir.getNomineeRequest != null) {
                                            orderServicesVM.NomiDir.getNomineeRequest.done(function () {
                                                if (orderServicesVM.NomiShare.getNomineeRequest != null) {
                                                    orderServicesVM.NomiShare.getNomineeRequest.done(function () {
                                                        $("#nominee").collapse("show")
                                                    })
                                                }
                                            })
                                        }
                                    } else {
                                        $("#nominee").collapse("hide")
                                    }
                                    break;
                                case"Trust":
                                    if (orderServicesVM.Trust.getCountryRequest != null) {
                                        orderServicesVM.Trust.getCountryRequest.done(function () {
                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Trust.key]) !== "undefined") {
                                                q.find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Trust.key]["detail_service_id"]]["service_country_id"]
                                                }).prop("selected", true)
                                            } else {
                                                if (typeof (orderServicesVM.globalData.sessions()["countryIdByGetRequest"]) !== "undefined" && orderServicesVM.globalData.sessions()["countryIdByGetRequest"] !== 0) {
                                                    q.find("option").filter(function () {
                                                        return $(this).prop("value") === orderServicesVM.globalData.sessions()["countryIdByGetRequest"]
                                                    }).prop("selected", true)
                                                }
                                            }
                                            orderServicesVM.globalData.numberOfTrustors(orderServicesVM.globalData.sessions()["numberOfTrustors"]);
                                            orderServicesVM.globalData.numberOfTrustees(orderServicesVM.globalData.sessions()["numberOfTrustees"]);
                                            q.trigger("change");
                                            if (orderServicesVM.Trust.getTypeRequest != null) {
                                                orderServicesVM.Trust.getTypeRequest.done(function () {
                                                    orderServicesVM.globalData.numberOfNomiTrustor(orderServicesVM.globalData.sessions()["numberOfNomiTrustors"]);
                                                    orderServicesVM.globalData.numberOfNomiTrustee(orderServicesVM.globalData.sessions()["numberOfNomiTrustees"]);
                                                    x.trigger("change");
                                                    b.trigger("change")
                                                })
                                            }
                                            if (orderServicesVM.ProcessingTrust.getProcessingRequest != null) {
                                                orderServicesVM.ProcessingTrust.getProcessingRequest.done(function () {
                                                    if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingTrust.key]) !== "undefined") {
                                                        j.find("input:radio").filter(function () {
                                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingTrust.key]["detail_service_id"]
                                                        }).prop("checked", true).trigger("click").trigger("change")
                                                    }
                                                })
                                            }
                                        })
                                    }
                                    if (orderServicesVM.globalData.selectNominee()) {
                                        if (orderServicesVM.NomiTrustor.getNomineeRequest != null) {
                                            orderServicesVM.NomiTrustor.getNomineeRequest.done(function () {
                                                if (orderServicesVM.NomiTrustee.getNomineeRequest != null) {
                                                    orderServicesVM.NomiTrustee.getNomineeRequest.done(function () {
                                                        $("#nominee").collapse("show")
                                                    })
                                                }
                                            })
                                        }
                                    } else {
                                        $("#nominee").collapse("hide")
                                    }
                                    break;
                                case"Foundation":
                                    if (orderServicesVM.Foundation.getCountryRequest != null) {
                                        orderServicesVM.Foundation.getCountryRequest.done(function () {
                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Foundation.key]) !== "undefined") {
                                                n.find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Foundation.key]["detail_service_id"]]["service_country_id"]
                                                }).prop("selected", true)
                                            } else {
                                                if (typeof (orderServicesVM.globalData.sessions()["countryIdByGetRequest"]) !== "undefined" && orderServicesVM.globalData.sessions()["countryIdByGetRequest"] !== 0) {
                                                    n.find("option").filter(function () {
                                                        return $(this).prop("value") === orderServicesVM.globalData.sessions()["countryIdByGetRequest"]
                                                    }).prop("selected", true)
                                                }
                                            }
                                            orderServicesVM.globalData.numberOfCouncils(orderServicesVM.globalData.sessions()["numberOfCouncils"]);
                                            orderServicesVM.globalData.numberOfFounders(orderServicesVM.globalData.sessions()["numberOfFounders"]);
                                            n.trigger("change");
                                            if (orderServicesVM.Foundation.getTypeRequest != null) {
                                                orderServicesVM.Foundation.getTypeRequest.done(function () {
                                                    orderServicesVM.globalData.numberOfNomiCouncil(orderServicesVM.globalData.sessions()["numberOfNomiCouncils"]);
                                                    orderServicesVM.globalData.numberOfNomiFounder(orderServicesVM.globalData.sessions()["numberOfNomiFounders"]);
                                                    r.trigger("change");
                                                    u.trigger("change")
                                                })
                                            }
                                            if (orderServicesVM.ProcessingFoundation.getProcessingRequest != null) {
                                                orderServicesVM.ProcessingFoundation.getProcessingRequest.done(function () {
                                                    if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingFoundation.key]) !== "undefined") {
                                                        z.find("input:radio").filter(function () {
                                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingFoundation.key]["detail_service_id"]
                                                        }).prop("checked", true).trigger("click").trigger("change")
                                                    }
                                                })
                                            }
                                        })
                                    }
                                    if (orderServicesVM.globalData.selectNominee()) {
                                        if (orderServicesVM.NomiCouncil.getNomineeRequest != null) {
                                            orderServicesVM.NomiCouncil.getNomineeRequest.done(function () {
                                                if (orderServicesVM.NomiFounder.getNomineeRequest != null) {
                                                    orderServicesVM.NomiFounder.getNomineeRequest.done(function () {
                                                        $("#nominee").collapse("show")
                                                    })
                                                }
                                            })
                                        }
                                    } else {
                                        $("#nominee").collapse("hide")
                                    }
                                    break;
                                case"Bank":
                                    if (orderServicesVM.Bank.getCountryRequest != null) {
                                        orderServicesVM.Bank.getCountryRequest.done(function () {
                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]) !== "undefined") {
                                                J.find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]["detail_service_id"]]["service_country_id"]
                                                }).prop("selected", true)
                                            } else {
                                                if (typeof (orderServicesVM.globalData.sessions()["countryIdByGetRequest"]) !== "undefined" && orderServicesVM.globalData.sessions()["countryIdByGetRequest"] !== 0) {
                                                    J.find("option").filter(function () {
                                                        return $(this).prop("value") === orderServicesVM.globalData.sessions()["countryIdByGetRequest"]
                                                    }).prop("selected", true)
                                                }
                                            }
                                            J.trigger("change");
                                            if (orderServicesVM.Bank.getBankRequest != null) {
                                                orderServicesVM.Bank.getBankRequest.done(function () {
                                                    if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]) !== "undefined") {
                                                        w.find("option").filter(function () {
                                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]["detail_service_id"]
                                                        }).prop("selected", true)
                                                    }
                                                    w.trigger("change");
                                                    if (orderServicesVM.ProcessingBank.getProcessingRequest != null) {
                                                        orderServicesVM.ProcessingBank.getProcessingRequest.done(function () {
                                                            if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingBank.key]) !== "undefined") {
                                                                C.find("input:radio").filter(function () {
                                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.ProcessingBank.key]["detail_service_id"]
                                                                }).prop("checked", true).trigger("click").trigger("change")
                                                            }
                                                        })
                                                    }
                                                })
                                            }
                                        })
                                    }
                                    if (orderServicesVM.globalData.bankWithSetupCompany()) {
                                        y.trigger("click");
                                        if (orderServicesVM.Incorporation.getCountryRequest != null) {
                                            orderServicesVM.Incorporation.getCountryRequest.done(function () {
                                                if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]) !== "undefined") {
                                                    K.find("option").filter(function () {
                                                        return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]["detail_service_id"]]["service_country_id"]
                                                    }).prop("selected", true)
                                                }
                                                K.trigger("change");
                                                if (orderServicesVM.Incorporation.getTypeRequest != null) {
                                                    orderServicesVM.Incorporation.getTypeRequest.done(function () {
                                                        if (typeof (orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]) !== "undefined") {
                                                            i.find("option").filter(function () {
                                                                return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.Incorporation.key]["detail_service_id"]
                                                            }).prop("selected", true)
                                                        }
                                                        orderServicesVM.globalData.numberOfDirectors(orderServicesVM.globalData.sessions()["numberOfDirectors"]);
                                                        orderServicesVM.globalData.numberOfShareholders(orderServicesVM.globalData.sessions()["numberOfShareholders"]);
                                                        orderServicesVM.globalData.numberOfNomiDir(orderServicesVM.globalData.sessions()["numberOfNomiDirectors"]);
                                                        orderServicesVM.globalData.numberOfNomiShare(orderServicesVM.globalData.sessions()["numberOfNomiShareholders"]);
                                                        $("#type-of-incorporate-2").trigger("change")
                                                    })
                                                }
                                            })
                                        }
                                    } else {
                                        if (orderServicesVM.globalData.bankWithoutSetupCompany()) {
                                            orderServicesVM.globalData.numberOfDirectors(orderServicesVM.globalData.sessions()["numberOfDirectors"]);
                                            orderServicesVM.globalData.numberOfShareholders(orderServicesVM.globalData.sessions()["numberOfShareholders"]);
                                            orderServicesVM.globalData.numberOfNomiDir(orderServicesVM.globalData.sessions()["numberOfNomiDirectors"]);
                                            orderServicesVM.globalData.numberOfNomiShare(orderServicesVM.globalData.sessions()["numberOfNomiShareholders"]);
                                            v.trigger("click");
                                            if (typeof (orderServicesVM.globalData.sessions()["countryIdOfIncorporationForOpenBank"]) !== "undefined") {
                                                $("#incorporation-country-3").find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["countryIdOfIncorporationForOpenBank"]
                                                }).prop("selected", true)
                                            }
                                            $("#type-of-incorporate-3").trigger("change")
                                        }
                                    }
                                    if (orderServicesVM.globalData.selectNominee()) {
                                        if (orderServicesVM.NomiDir.getNomineeRequest != null) {
                                            orderServicesVM.NomiDir.getNomineeRequest.done(function () {
                                                if (orderServicesVM.NomiShare.getNomineeRequest != null) {
                                                    orderServicesVM.NomiShare.getNomineeRequest.done(function () {
                                                        $("#nominee").collapse("show")
                                                    })
                                                }
                                            })
                                        }
                                    } else {
                                        $("#nominee").collapse("hide")
                                    }
                                    break;
                                default:
                                    break
                            }
                            if (orderServicesVM.globalData.selectBank()) {
                                if (orderServicesVM.Bank.getCountryRequest != null) {
                                    orderServicesVM.Bank.getCountryRequest.done(function () {
                                        m.find("option").filter(function () {
                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]["detail_service_id"]]["service_country_id"]
                                        }).prop("selected", true);
                                        $("#open-bank").collapse("show");
                                        if (orderServicesVM.Bank.getBankRequest != null) {
                                            orderServicesVM.Bank.getBankRequest.done(function () {
                                                h.find("option").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bank.key]["detail_service_id"]
                                                }).prop("selected", true);
                                                h.trigger("change")
                                            })
                                        }
                                    })
                                }
                            } else {
                                $("#open-bank").collapse("hide")
                            }
                            if (orderServicesVM.globalData.selectOffice()) {
                                if (orderServicesVM.OfficePhone.getCountryRequest != null) {
                                    orderServicesVM.OfficePhone.getCountryRequest.done(function () {
                                        H.find("option").filter(function () {
                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["servicesDetails"][orderServicesVM.globalData.sessions()["services"][orderServicesVM.OfficePhone.key]["detail_service_id"]]["service_country_id"]
                                        }).prop("selected", true);
                                        t.prop("checked", true).trigger("change");
                                        if (orderServicesVM.OfficePhone.getOfficeRequest != null) {
                                            orderServicesVM.OfficePhone.getOfficeRequest.done(function () {
                                                M.find("input:radio").filter(function () {
                                                    return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.OfficePhone.key]["detail_service_id"]
                                                }).prop("checked", true).trigger("click").trigger("change")
                                            })
                                        }
                                    })
                                }
                            } else {
                                t.prop("checked", false).trigger("change")
                            }
                            if (defaultCheckedServiceOffice) {
                                if (orderServicesVM.OfficePhone.getCountryRequest != null) {
                                    orderServicesVM.OfficePhone.getCountryRequest.done(function () {
                                        H.find("option").filter(function () {
                                            return $(this).prop("value") === defaultCountryIdForServiceOffice
                                        }).prop("selected", true);
                                        t.prop("checked", true).trigger("change")
                                    })
                                }
                            }
                            if (orderServicesVM.globalData.selectMerchantAccountOnline() || defaultCheckedMerchant) {
                                if (orderServicesVM.Merchant.getMerchantRequest != null) {
                                    orderServicesVM.Merchant.getMerchantRequest.done(function () {
                                        if (orderServicesVM.Merchant.isActive() && orderServicesVM.Merchant.noOfMerchant()) {
                                            F.prop("checked", true).trigger("change")
                                        } else {
                                            F.prop("checked", false).trigger("change")
                                        }
                                    })
                                }
                            } else {
                                F.prop("checked", false).trigger("change")
                            }
                            if (orderServicesVM.globalData.selectIpServices() || defaultCheckedIPService) {
                                if (orderServicesVM.IPService.getIPServiceRequest != null) {
                                    orderServicesVM.IPService.getIPServiceRequest.done(function () {
                                        if (orderServicesVM.IPService.isActive() && orderServicesVM.IPService.noOfIPService()) {
                                            f.prop("checked", true).trigger("change")
                                        } else {
                                            f.prop("checked", false).trigger("change")
                                        }
                                    })
                                }
                            } else {
                                f.prop("checked", false).trigger("change")
                            }
                            if (orderServicesVM.globalData.selectAccounting() || defaultCheckedAccounting) {
                                if (orderServicesVM.Accounting.getAccountingRequest != null) {
                                    orderServicesVM.Accounting.getAccountingRequest.done(function () {
                                        if (orderServicesVM.Accounting.isActive() && orderServicesVM.Accounting.noOfAccounting()) {
                                            B.prop("checked", true).trigger("change")
                                        } else {
                                            B.prop("checked", false).trigger("change")
                                        }
                                    })
                                }
                            } else {
                                B.prop("checked", false).trigger("change")
                            }
                            if (orderServicesVM.globalData.selectBookkeeping()) {
                                if (orderServicesVM.Bookkeeping.getBookkeepingRequest != null) {
                                    orderServicesVM.Bookkeeping.getBookkeepingRequest.done(function () {
                                        g.find("option").filter(function () {
                                            return $(this).prop("value") === orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bookkeeping.key]["detail_service_id"]
                                        }).prop("selected", true);
                                        orderServicesVM.globalData.bookkeepingPeriodValue(orderServicesVM.globalData.sessions()["services"][orderServicesVM.Bookkeeping.key]["detail_service_price_type"]);
                                        N.prop("checked", true).trigger("change")
                                    })
                                }
                            } else {
                                N.prop("checked", false).trigger("change")
                            }
                            if (defaultCheckedBookkeeping) {
                                if (orderServicesVM.Bookkeeping.getBookkeepingRequest != null) {
                                    orderServicesVM.Bookkeeping.getBookkeepingRequest.done(function () {
                                        g.find("option").filter(function () {
                                            return $(this).prop("value") === defaultBookkeepingIdForBookkeeping
                                        }).prop("selected", true);
                                        N.prop("checked", true).trigger("change")
                                    })
                                }
                            }
                            if (defaultCheckedNominee) {
                                $("#nominee").collapse("show")
                            }
                            $("#show-promotion-code").collapse("show")
                        });
                        $("select").change(function (P) {
                            var Q = $(this);
                            if (Q.val() !== "") {
                                Q.parent().removeClass("has-error")
                            }
                        });
                        $("#step1-form").submit(function (R) {
                            $("#load-form-message").hide();
                            $("#bank-question-error").hide();
                            var S = "<strong>Please review and correct the field(s) marked in red!</strong>";
                            var Q = $("#error-message, #error-message-sidebar");
                            Q.hide();
                            $("#step1-form *").removeClass("has-error");
                            var P = false;
                            switch (e.prop("value")) {
                                case"":
                                    e.parent().addClass("has-error");
                                    P = true;
                                    break;
                                case"Company":
                                    if ($("#incorporation-country").prop("value") < 1) {
                                        $("#incorporation-country").parent().addClass("has-error");
                                        P = true
                                    }
                                    if ($("#type-of-incorporate").prop("value") < 1) {
                                        $("#type-of-incorporate").parent().addClass("has-error");
                                        P = true
                                    } else {
                                        if (orderServicesVM.globalData.incorporationProcessingID() < 1) {
                                            S = "<strong>Please choose Processing Time!</strong>";
                                            P = true
                                        }
                                    }
                                    break;
                                case"Trust":
                                    if (q.prop("value") < 1) {
                                        q.parent().addClass("has-error");
                                        P = true
                                    } else {
                                        if (orderServicesVM.globalData.trustProcessingID() < 1) {
                                            S = "<strong>Please choose Processing Time!</strong>";
                                            P = true
                                        }
                                    }
                                    break;
                                case"Foundation":
                                    if (n.prop("value") < 1) {
                                        n.parent().addClass("has-error");
                                        P = true
                                    } else {
                                        if (orderServicesVM.globalData.foundationProcessingID() < 1) {
                                            S = "<strong>Please choose Processing Time!</strong>";
                                            P = true
                                        }
                                    }
                                    break;
                                case"Bank":
                                    if (J.prop("value") < 1) {
                                        J.parent().addClass("has-error");
                                        P = true
                                    }
                                    if (w.prop("value") < 1) {
                                        w.parent().addClass("has-error");
                                        P = true
                                    } else {
                                        if (orderServicesVM.globalData.bankProcessingID() < 1) {
                                            S = "<strong>Please choose Processing Time!</strong>";
                                            P = true
                                        }
                                    }
                                    if (orderServicesVM.globalData.bankWithSetupCompany()) {
                                        if ($("#incorporation-country-2").prop("value") < 1) {
                                            $("#incorporation-country-2").parent().addClass("has-error");
                                            P = true
                                        }
                                        if ($("#type-of-incorporate-2").prop("value") < 1) {
                                            $("#type-of-incorporate-2").parent().addClass("has-error");
                                            P = true
                                        }
                                    } else {
                                        if (orderServicesVM.globalData.bankWithoutSetupCompany()) {
                                            if ($("#incorporation-country-3").prop("value") < 1) {
                                                $("#incorporation-country-3").parent().addClass("has-error");
                                                P = true
                                            }
                                        } else {
                                            $("#bank-question-error").show();
                                            P = true
                                        }
                                    }
                                    break;
                                default:
                                    S = "Invalid data!";
                                    P = true;
                                    break
                            }
                            if (e.prop("value") !== "Bank" && k.is(":checked")) {
                                if (m.prop("value") < 1) {
                                    m.parent().addClass("has-error");
                                    P = true
                                }
                                if (h.prop("value") < 1) {
                                    h.parent().addClass("has-error");
                                    P = true
                                }
                            }
                            if (t.is(":checked")) {
                                if (H.prop("value") < 1) {
                                    H.parent().addClass("has-error");
                                    P = true
                                } else {
                                    if (orderServicesVM.globalData.officeID() < 1) {
                                        S = "<strong>Please choose Package Fee!<strong>";
                                        P = true
                                    }
                                }
                            }
                            if (N.is(":checked")) {
                                if (g.prop("value") < 1) {
                                    g.parent().addClass("has-error");
                                    P = true
                                } else {
                                    if (orderServicesVM.globalData.bookkeepingPeriodValue() === "") {
                                        S = "<strong>Please choose report of bookkeeping by monthly or quarterly!<strong>";
                                        P = true
                                    }
                                }
                            }
                            if (P) {
                                R.preventDefault();
                                Q.html(S);
                                Q.show()
                            } else {
                                lLoading.show()
                            }
                        });
                        $("#promotion-submit").on("click", function (Q) {
                            orderServicesVM.globalData.promotionMessage("");
                            var P = E.prop("value");
                            orderServicesVM.globalData.applyPromotion(P);
                            Q.preventDefault()
                        })
                    })
                });

                function resetProcessingTimeOfIncorporation() {
                    orderServicesVM.ProcessingIncorporation.getProcessings(0, 0);
                    orderServicesVM.globalData.incorporationProcessingID(0);
                    orderServicesVM.globalData.incorporationProcessingName("Please select");
                    orderServicesVM.globalData.incorporationProcessingPrice(0)
                }

                function resetProcessingTimeOfBank() {
                    orderServicesVM.ProcessingBank.getProcessings(0, 0);
                    orderServicesVM.globalData.bankProcessingID(0);
                    orderServicesVM.globalData.bankProcessingName("Please select");
                    orderServicesVM.globalData.bankProcessingPrice(0)
                }

                function resetProcessingTimeOfTrust() {
                    orderServicesVM.ProcessingTrust.getProcessings(0, 0);
                    orderServicesVM.globalData.trustProcessingID(0);
                    orderServicesVM.globalData.trustProcessingName("Please select");
                    orderServicesVM.globalData.trustProcessingPrice(0)
                }

                function resetProcessingTimeOfFoundation() {
                    orderServicesVM.ProcessingFoundation.getProcessings(0, 0);
                    orderServicesVM.globalData.foundationProcessingID(0);
                    orderServicesVM.globalData.foundationProcessingName("Please select");
                    orderServicesVM.globalData.foundationProcessingPrice(0)
                }

                function resetDirectors() {
                    orderServicesVM.globalData.isDirector(false);
                    orderServicesVM.globalData.numberOfDirectors(0);
                    $("#number-of-directors").html(createNumberOptions(0, 0, 0));
                    $("#number-of-directors").trigger("change")
                }

                function resetShareholders() {
                    orderServicesVM.globalData.isShareholder(false);
                    orderServicesVM.globalData.numberOfShareholders(0);
                    $("#number-of-shareholders").html(createNumberOptions(0, 0, 0));
                    $("#number-of-shareholders").trigger("change")
                }

                function resetTrustors() {
                    orderServicesVM.Trust.isTrustor(false);
                    orderServicesVM.globalData.isTrustor(false);
                    orderServicesVM.globalData.numberOfTrustors(0);
                    $("#number-of-trustors").html(createNumberOptions(0, 0, 0));
                    $("#number-of-trustors").trigger("change")
                }

                function resetTrustees() {
                    orderServicesVM.Trust.isTrustee(false);
                    orderServicesVM.globalData.isTrustee(false);
                    orderServicesVM.globalData.numberOfTrustees(0);
                    $("#number-of-trustees").html(createNumberOptions(0, 0, 0));
                    $("#number-of-trustees").trigger("change")
                }

                function resetCouncils() {
                    orderServicesVM.Foundation.isCouncil(false);
                    orderServicesVM.globalData.isCouncil(false);
                    orderServicesVM.globalData.numberOfCouncils(0);
                    $("#number-of-councils").html(createNumberOptions(0, 0, 0));
                    $("#number-of-councils").trigger("change")
                }

                function resetFounders() {
                    orderServicesVM.Foundation.isFounder(false);
                    orderServicesVM.globalData.isFounder(false);
                    orderServicesVM.globalData.numberOfFounders(0);
                    $("#number-of-founders").html(createNumberOptions(0, 0, 0));
                    $("#number-of-founders").trigger("change")
                }

                function resetTypeOfIncorporation() {
                    orderServicesVM.globalData.typeOfIncorporationID(0);
                    orderServicesVM.globalData.typeOfIncorporationName("Please select");
                    orderServicesVM.globalData.typeOfIncorporationPrice(0)
                }

                function resetTypeOfTrust() {
                    orderServicesVM.globalData.typeOfTrustID(0);
                    orderServicesVM.globalData.typeOfTrustName("Please select");
                    orderServicesVM.globalData.typeOfTrustPrice(0)
                }

                function resetTypeOfFoundation() {
                    orderServicesVM.globalData.typeOfFoundationID(0);
                    orderServicesVM.globalData.typeOfFoundationName("Please select");
                    orderServicesVM.globalData.typeOfFoundationPrice(0)
                }

                function resetTypeOfBank() {
                    orderServicesVM.globalData.bankID(0);
                    orderServicesVM.globalData.bankName("Please select");
                    orderServicesVM.globalData.bankPrice(0)
                }

                function resetCountryOfIncorporation() {
                    orderServicesVM.globalData.incorporationCountryID(0);
                    orderServicesVM.globalData.incorporationCountryName("Please select")
                }

                function resetCountryOfTrust() {
                    orderServicesVM.globalData.trustCountryID(0);
                    orderServicesVM.globalData.trustCountryName("Please select")
                }

                function resetCountryOfFoundation() {
                    orderServicesVM.globalData.foundationCountryID(0);
                    orderServicesVM.globalData.foundationCountryName("Please select")
                }

                function resetCountryOfBank() {
                    orderServicesVM.globalData.bankCountryID(0);
                    orderServicesVM.globalData.bankCountryName("Please select")
                }

                function resetCountryOfOfficePhone() {
                    orderServicesVM.globalData.officeCountryID(0);
                    orderServicesVM.globalData.officeCountryName("Please select");
                    orderServicesVM.globalData.officeName("Please select");
                    orderServicesVM.globalData.officePrice(0)
                }

                function resetBookkeepings() {
                    orderServicesVM.globalData.bookkeepingID(0);
                    orderServicesVM.globalData.bookkeepingName("Please select");
                    orderServicesVM.globalData.bookkeepingPeriodName("Please select");
                    orderServicesVM.globalData.bookkeepingQuarterPrice(0);
                    orderServicesVM.globalData.bookkeepingMonthPrice(0);
                    orderServicesVM.globalData.bookkeepingPrice(0)
                }

                function resetNominee() {
                    orderServicesVM.globalData.numberOfNomiShare(0);
                    orderServicesVM.globalData.totalOfNomiShare(0);
                    orderServicesVM.globalData.numberOfNomiDir(0);
                    orderServicesVM.globalData.totalOfNomiDir(0);
                    orderServicesVM.globalData.numberOfNomiTrustor(0);
                    orderServicesVM.globalData.totalOfNomiTrustor(0);
                    orderServicesVM.globalData.numberOfNomiTrustee(0);
                    orderServicesVM.globalData.totalOfNomiTrustee(0);
                    orderServicesVM.globalData.numberOfNomiCouncil(0);
                    orderServicesVM.globalData.totalOfNomiCouncil(0);
                    orderServicesVM.globalData.numberOfNomiFounder(0);
                    orderServicesVM.globalData.totalOfNomiFounder(0)
                }

                var createNumberOptions = function (c, a, e) {
                    var d = "";
                    for (var b = c; b <= a; b++) {
                        d += "<option value='" + b + "'" + (b == e || (b == a && b < e) ? "selected" : "") + ">" + b + "</option>"
                    }
                    return d
                };
                var numberOfNominee = function (d, c) {
                    var b = "";
                    for (var a = 0; a <= d; a++) {
                        b += "<option value='" + a + "'" + (a == c || (a == d && a < c) ? "selected" : "") + ">" + a + "</option>"
                    }
                    return b
                }; </script>
            </div>
        </div>
    </div>
</div>
